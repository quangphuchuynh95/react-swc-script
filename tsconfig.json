{
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "es2016",
    "lib": [
      "ESNext"
    ],
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}
